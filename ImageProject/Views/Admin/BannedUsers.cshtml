@{
    ViewData["Title"] = "AdminPanel";
}

<partial name="_AdminHeader" />
<div id="partialPlace">
    <table class="table">
        <tr>
            <th>Name</th>
            <th></th>
        </tr>
        @foreach (var bUser in Model)
        {
            <tr>
                <td>@bUser.UserName</td>
                <td>
                    <form asp-action="UnBanUser" asp-route-id="@bUser.Id" method="post">
                        <button type="submit" class="btn btn-sm btn-success">UnBan</button>
                    </form>
                </td>
            </tr>
        }
    </table>
</div>

