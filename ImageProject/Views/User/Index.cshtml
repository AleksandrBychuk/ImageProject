@model IEnumerable<ImageProject.Models.UserImage>

<div>
    <p>@Model.FirstOrDefault().UserOwner.UserName</p>
    @foreach (var item in Model)
    {
        @Html.Raw("<img style='width:500px; height:500px;' src=\"data:image/jpeg;base64," + Convert.ToBase64String(item.Image) + "\" />")
    }
</div>
